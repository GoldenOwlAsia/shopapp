{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-unexpected-multiline": "error",
    "react/jsx-filename-extension": [
      0
    ],
    "react/destructuring-assignment": [
      0
    ],
    "react/state-in-constructor": [
      0
    ],
    "max-classes-per-file": [
      0
    ],
    "react/jsx-props-no-spreading": [
      "warn"
    ]
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ]
}